def previous_greater_elements(arr):
    stack = []
    result = []

    for num in arr:
        
        while stack and stack[-1] <= num:
            stack.pop()

        
        if not stack:
            result.append(-1)
        else:
            result.append(stack[-1])

        
        stack.append(num)

    return result



arr = [4, 10, 5, 8, 20, 15, 3, 12]
print(previous_greater_elements(arr))
